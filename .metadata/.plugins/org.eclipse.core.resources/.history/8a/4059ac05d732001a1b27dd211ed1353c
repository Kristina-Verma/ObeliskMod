package fox.obeliskmod.lists;

import fox.obeliskmod.ObeliskMod;
import net.minecraft.entity.EntityType;
import net.minecraft.item.Item;
import net.minecraft.item.SpawnEggItem;

public class EntitiesList 
{
	public static EntityType<?> TEST_ENTITY;
	
	public static Item registerEntitySpawnEgg(EntityType<?> type, int color1, int color2, String name) 
	{
		SpawnEggItem item = new SpawnEggItem(type, color1, color2, new Item.Properties().group(ObeliskMod.obelisk));
		item.setRegistryName(ObeliskMod.location(name));
	}
}
